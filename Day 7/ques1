//House Robber (LeetCode)

class Solution {
    public int rob(int[] nums) {
        int n=nums.length;
        if(n==1){
            return nums[0];
        }
        nums[1]=Math.max(nums[0], nums[1]);
        for(int i=2;i<n;i++){
            int max=nums[i]+ nums[i-2];
            if(max>nums[i-1]){
                nums[i]=max;
            }
            else{
                nums[i]= nums[i-1];
            }
        }
        return nums[n-1];
    }
}
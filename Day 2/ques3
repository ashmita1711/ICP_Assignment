//Valid Perfect Square(LeetCode Q-367)

//Brute Force Approach

class Solution {
    public boolean isPerfectSquare(int num) {
        if (num == 1) return true;
        for (long i = 1; i * i <= num; i++) {
            if (i * i == num) {
                return true;
            }
        }
        return false;
    }
}


//Optimal Approach